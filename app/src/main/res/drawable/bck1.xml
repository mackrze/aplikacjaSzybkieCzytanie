<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle" >
    <corners
        android:radius="10dp"
        />
    <gradient
        android:gradientRadius="170dp"
        android:centerX="50%"
        android:centerY="50%"
        android:centerColor="#EAF8CB88"
        android:startColor="#EEFAFAFA"
        android:endColor="#ECBF7201"
        android:type="radial"

        />
    <padding
        android:left="0dp"
        android:top="8dp"
        android:right="0dp"
        android:bottom="0dp"
        />
    <size
        android:width="250dp"
        android:height="60dp"
        />
    <stroke
        android:width="0dp"
        android:color="#FFFFFF"
        />
</shape>
